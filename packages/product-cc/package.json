{
  "name": "product-cc",
  "version": "0.1.0",
  "description": "Chaincodes package for product",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": { 
    "clean": "rimraf dist client", 
    "build": "npm run clean && tsc", 
    "prepare": "npm run build", 
    "test": "npm run build && mocha -r ts-node/register tests/*.spec.ts --reporter spec",
    "test:e2e": "npm run build && mocha -r ts-node/register -t 300000 tests/*.e2e.ts --reporter spec",
    "test:e2e:forma": "npm run build && mocha --inspect --debug=8888 -r ts-node/register -t 300000 tests/*.forma.e2e.ts --reporter spec"
  },
  "dependencies": { 
    "yup": "^0.26.6", 
    "reflect-metadata": "^0.1.12", 
    "@worldsibu/convector-core": "1.2.1-0-alpha.1624740f",
    "@worldsibu/convector-platform-fabric": "1.2.1-0-alpha.1624740f"
  },
  "devDependencies": {  
    "@types/node": "^10.12.5",  
    "@worldsibu/convector-storage-couchdb": "1.2.1-0-alpha.1624740f",
    "rimraf": "^2.6.2",  
    "ts-node": "^8.0.2",  
    "mocha": "^5.0.3",  
    "chai": "^4.1.2",  
    "@types/mocha": "^5.2.5",  
    "@types/chai": "^4.1.4" 
  } 
}

